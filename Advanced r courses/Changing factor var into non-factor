#Factor variable Trap converting factor to non-factor
  #converting characters in numeric
a <- c("12","13","14","12","12")
typeof(a)
b <- as.numeric(a)
b
typeof(b)
  #Converting into factor into numeric
z <- factor(c("12","13","14","12","12"))
z
typeof(z)
y <- as.numeric(z)
y
typeof(y)
#-------------------------------Correct way:
x <- as.numeric(as.character(z))
x
typeof(x)
